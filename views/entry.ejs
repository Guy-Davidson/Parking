<html>
    <body>                   
        <h1>Click OK to enter</h1>
        <hr>
        <form method="post" id="entry_form">
            <p>plate number: <%= plate %></p>
            <button type="submit">OK</button>
        </form>
    </body>
    <script defer>
        const plate = '<%- JSON.stringify(plate) %>'           
        document.getElementById('entry_form').action = `/entry?plate=${plate}&parkingLot=382`
    </script>
</html>